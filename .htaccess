RewriteEngine On
# Root â†’ /assessment/
RewriteRule ^$ https://intellismartai.com/assessment/ [R=301,L]
RewriteRule ^index\.html$ https://intellismartai.com/assessment/ [R=301,L]
# Preserve API/image paths
RewriteRule ^api/(.*)$ https://intellismartai.com/assessment/api/$1 [R=301,L]
